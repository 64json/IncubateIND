    "faq": {
        "application": {
            "title": "Application",
            "contents": {
                "$type": "array_cols",
                "$value": [
                    {
                        "question": "Who can apply?",
                        "answer": "Any high school or college student can apply to participate in IncubateIND, regardless of experience with programming. Because it is an entrepreneurship event, we are looking for a diverse pool of participants in terms of skillsets."
          },
                    {
                        "question": "How do I apply?",
                        "answer": "Just fill out the registration form! More info will be sent out later."
          },
                    {
                        "question": "What is the application deadline?",
                        "answer": "This will be decided later. The sooner you apply, the better!"
          }
        ],
                "$options": {
                    "colsInRow": 3
                }
            }
        },
        "logistics": {
            "title": "Logistics",
            "contents": {
                "$type": "array_cols",
                "$value": [
                    {
                        "question": "When/Where?",
                        "answer": "IncubateIND will take place on August 6-7 at TBD."
          },
                    {
                        "question": "Is there transportation or travel reimbursement?",
                        "answer": "No"
          },
                    {
                        "question": "What should I bring?",
                        "answer": "Bring your laptop, toiletries, a sleeping bag, and any other technology that you want to hack with. You <strong>don't</strong> need a predetermined team or a product idea."
          }
        ],
                "$options": {
                    "colsInRow": 3
                }
            }
        },
        "hacking": {
            "title": "Hacking",
            "contents": {
                "$type": "array_cols",
                "$value": [
                    {
                        "question": "How much time will we have to hack?",
                        "answer": "Teams will have 30 hours to build their products."
          },
                    {
                        "question": "Will there be hardware or API keys provided?",
                        "answer": "Yes. We will release a list of hardware and API's later."
          },
                    {
                        "question": "Do I need to be able to code?",
                        "answer": "No! Participants will be able to act as product managers or designers for their teams, or play other roles. If you don't know how to code but want to learn, you can attend one of our various workshops."
          }
        ],
                "$options": {
                    "colsInRow": 3
                }
            }
        },
        "other": {
            "title": "Other",
            "contents": {
                "$type": "array_cols",
                "$value": [
                    {
                        "question": "How much does the event cost?",
                        "answer": "It's free! We will also provide meals, snacks, and refreshments throughout the event."
          },
                    {
                        "question": "Is there a Code of Conduct?",
                        "answer": "Yes, <a href='http://static.mlh.io/docs/mlh-code-of-conduct.pdf' target='_blank'>here</a>. <br>tl;dr respect other participants and don't be a jerk."
          },
                    {
                        "question": "Will there be prizes?",
                        "answer": "Yes. We will have both physical prizes and opportunities with sponsoring companies. A full list of prizes will be released later."
          }
        ],
                "$options": {
                    "colsInRow": 3
                }
            }
        }
    },
